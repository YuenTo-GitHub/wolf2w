<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wolfcode.wolf2w.article.mapper.TravelMapper">

    <update id="numIncr">
        update travel
        set ${column} = ${column} + #{increment}
        where id = #{tid}
    </update>

</mapper>